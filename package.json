{
  "name": "Good-Party",
  "version": "1.0.0",
  "description": "Power and money have corrupted both major U.S. Political parties.We're working on a truly Good alternative!",
  "author": "Tomer Almog",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "dev": "API_ENV=local next dev -p 4000",
    "dev-dev": "API_ENV=development next dev -p 4000",
    "dev-dev-pm2": "pm2 start 'npm run dev-dev'",
    "dev-test": "cross-env CYPRESS_BASE_URL=https://dev.goodparty.org CYPRESS_API_BASE=https://api-dev.goodparty.org/api/v1/ ./node_modules/.bin/cypress run",
    "dev-prod": "API_ENV=prod next dev -p 4000",
    "deploy-dev": "git checkout develop && eb deploy",
    "deploy-qa": "git checkout qa && eb deploy",
    "deploy-prod": "git checkout master  && eb deploy",
    "build": "next build",
    "generate": "plop --plopfile internals/generators/index.js",
    "start": "next build && next start",
    "lint": "next lint",
    "cypress": "./node_modules/.bin/cypress run",
    "cypress:local": "cross-env CYPRESS_BASE_URL=http://localhost:4000  CYPRESS_API_BASE=http://localhost:1337/api/v1/ ./node_modules/.bin/cypress open",
    "cypress:local-dev": "cross-env CYPRESS_BASE_URL=http://localhost:4000 CYPRESS_API_BASE=https://api-dev.goodparty.org/api/v1/ ./node_modules/.bin/cypress open --config defaultCommandTimeout=100000",
    "cypress:local-prod": "cross-env CYPRESS_BASE_URL=http://localhost:4000 CYPRESS_API_BASE=https://api.goodparty.org/api/v1/ ./node_modules/.bin/cypress open",
    "cypress:dev": "cross-env CYPRESS_BASE_URL=https://dev.goodparty.org ./node_modules/.bin/cypress open",
    "cypress:prod": "cross-env CYPRESS_BASE_URL=https://goodparty.org ./node_modules/.bin/cypress open",
    "cypress:run": "cross-env CYPRESS_BASE_URL=http://localhost:3000 ./node_modules/.bin/cypress run",
    "cypress:run:local": "cross-env CYPRESS_BASE_URL=http://localhost:4000 ./node_modules/.bin/cypress run",
    "cypress:run:dev": "cross-env CYPRESS_BASE_URL=https://dev.goodparty.org ./node_modules/.bin/cypress run",
    "cypress:run:prod": "cross-env CYPRESS_BASE_URL=https://goodparty.org ./node_modules/.bin/cypress run",
    "cypress:run:test": "cross-env CYPRESS_BASE_URL=https://test.goodparty.org ./node_modules/.bin/cypress run"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^15.11.1",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@next/bundle-analyzer": "^12.1.6",
    "axios": "^0.21.2",
    "connected-next-router": "^4.1.0",
    "connected-react-router": "^6.9.2",
    "cross-env": "^7.0.3",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "eslint-config-next": "12.0.10",
    "ga-4-react": "^0.1.281",
    "hamburger-react": "^2.5.0",
    "html-to-image": "^1.9.0",
    "immer": "^9.0.12",
    "invariant": "^2.2.4",
    "jodit-react": "^1.1.21",
    "libphonenumber-js": "1.9.48",
    "markdown-to-jsx": "^7.0.0",
    "material-ui-color-picker": "^3.5.1",
    "moment": "^2.29.2",
    "next": "12.1.0",
    "next-compose-plugins": "^2.2.1",
    "next-pwa": "^5.4.4",
    "next-redux-wrapper": "^7.0.5",
    "node-fetch": "^3.2.0",
    "pm2": "^5.2.0",
    "react": "17.0.2",
    "react-code-blocks": "0.0.9-0",
    "react-copy-to-clipboard": "^5.0.4",
    "react-csv": "^2.2.2",
    "react-dom": "17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.3.1",
    "react-image-crop": "^9.1.1",
    "react-lazy-load-image-component": "^1.5.1",
    "react-lite-youtube-embed": "^2.2.0",
    "react-number-format": "^4.4.1",
    "react-phone-input-2": "^2.15.0",
    "react-player": "^2.9.0",
    "react-redux": "^7.2.6",
    "react-router": "^6.2.1",
    "react-scroll": "^1.8.4",
    "react-slick": "^0.28.1",
    "react-social-login": "^3.4.15",
    "react-star-rating-component": "^1.4.1",
    "react-sticky-el": "^2.0.9",
    "react-table": "^6.11.5",
    "react-truncate": "^2.4.0",
    "recharts": "^2.1.8",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga": "^1.1.3",
    "reselect": "^4.1.5",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "concurrently": "^7.2.2",
    "cypress": "4.10.0",
    "cypress-promise": "^1.1.0",
    "eslint": "8.8.0",
    "node-plop": "0.18.0",
    "plop": "2.3.0",
    "prettier": "^2.5.1"
  }
}
