{
  "name": "Good-Party",
  "version": "1.0.0",
  "description": "Power and money have corrupted both major U.S. Political parties.We're working on a truly Good alternative!",
  "engines": {
    "npm": ">=5",
    "node": ">=8.15.1"
  },
  "author": "Tomer Almog",
  "license": "MIT",
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "extract-intl": "node ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "prebuild": "npm run build:clean",
    "build-old": "npm run prebuild && cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "build-analyze": "npm run prebuild && cross-env NODE_ENV=production ANALYZE=yes webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "aws-eb:prod": "npm run build && npm run start:prod",
    "start-old": "npm run build && npm run start:prod",
    "build": "next build",
    "start": "next build && NODE_ENV=production next start",
    "dev": "cross-env API_ENV=local next dev -p 4000",
    "dev-old": "cross-env NODE_ENV=development API_ENV=local node server/start",
    "dev-dev": "cross-env NODE_ENV=development API_ENV=dev node server/start",
    "dev-prod": "cross-env NODE_ENV=development API_ENV=prod node server/start",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server/start",
    "start:production": "npm run build && npm run start:prod",
    "start:production-test": "npm run test && npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server/start",
    "presetup": "npm i chalk shelljs",
    "setup": "node ./internals/scripts/setup.js",
    "clean": "shjs ./internals/scripts/clean.js",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint old-app/**/*.js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettify": "prettier --write",
    "deploy-dev": "git checkout develop  && eb deploy",
    "deploy-prod": "git checkout master  && eb deploy",
    "cypress:local": "cross-env CYPRESS_BASE_URL=http://localhost:4000  CYPRESS_API_BASE=http://localhost:1337/api/v1/ ./node_modules/.bin/cypress open",
    "cypress:local-dev": "cross-env CYPRESS_BASE_URL=http://localhost:4000 CYPRESS_API_BASE=https://api-dev.thegoodparty.org/api/v1/ ./node_modules/.bin/cypress open",
    "cypress:local-prod": "cross-env CYPRESS_BASE_URL=http://localhost:4000 CYPRESS_API_BASE=https://api.thegoodparty.org/api/v1/ ./node_modules/.bin/cypress open",
    "cypress:dev": "cross-env CYPRESS_BASE_URL=https://dev.thegoodparty.org ./node_modules/.bin/cypress open",
    "cypress:prod": "cross-env CYPRESS_BASE_URL=https://thegoodparty.org ./node_modules/.bin/cypress open",
    "cypress:run:local": "cross-env CYPRESS_BASE_URL=http://localhost:4000 ./node_modules/.bin/cypress run",
    "cypress:run:dev": "cross-env CYPRESS_BASE_URL=https://dev.thegoodparty.org ./node_modules/.bin/cypress run",
    "cypress:run:prod": "cross-env CYPRESS_BASE_URL=https://thegoodparty.org ./node_modules/.bin/cypress run",
    "cypress:run:test": "cross-env CYPRESS_BASE_URL=https://test.thegoodparty.org ./node_modules/.bin/cypress run"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 10"
  ],
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
